!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
!! Rofi

rofi.color-enabled:	true
rofi.color-window:	#000000, #00ff00, #00cc00
rofi.color-normal:	#000000, #00ff00, #000000, #006600, #00ff00
rofi.color-active:	#000000, #ffff00, #000000, #006600, #ffff00
rofi.color-urgent:	#000000, #ff6600, #000000, #006600, #ff6600
rofi.fake-transparency:	true
rofi.levenshtein-sort:	true
rofi.separator-style:	solid

!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
!! XTerm

! basics
XTerm*font:		7x13
XTerm*background:	black
XTerm*foreground:	grey
XTerm*geometry:	160x60+50+50

! bold by color
XTerm*boldMode:	false
XTerm*boldFont:	7x13
XTerm*colorBDMode:	true
XTerm*colorBD:		white

! Cygwin-style
XTerm*colorULMode:	true
XTerm*colorUL:		dark cyan

! misc
XTerm*metaSendsEscape:	true
XTerm*rightScrollBar:	true
XTerm*saveLines:	10000
XTerm*scrollBar:	true
XTerm*toolBar:		false

!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
!! URxvt

URxvt*termName:	rxvt-256color

URxvt*font:		7x13
URxvt*geometry:	160x60+50+50

URxvt*scrollstyle:	plain
URxvt*scrollBar_right:	true

URxvt*background:	black
URxvt*foreground:	grey
URxvt*color4:		rgb:33/33/ff
URxvt*color12:		rgb:66/66/ff
URxvt*colorBD:		white
URxvt*colorUL:		dark cyan

!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
!! misc

Emacs*font:		7x13
XLock.mode:		blank

