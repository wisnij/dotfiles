  ;; Your init file should contain only one such instance.
 '(diff-added ((t (:inherit diff-changed :foreground "blue"))))
 '(diff-added-face ((t (:inherit diff-changed :foreground "blue"))))
 '(diff-changed ((nil (:foreground "darkgreen"))))
